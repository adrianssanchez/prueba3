@model IEnumerable<AppGestionEMS.Models.Matriculas>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Curso.actual)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Grupo.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fecha)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Curso.actual)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Grupo.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fecha)
        </td>
        <td>
            @Html.ActionLink("Delete", "Delete", new { curso = item.CursoId, grupo = item.GrupoId, user = item.UserId })
        </td>
    </tr>
}

</table>
